# Unity WebGL Build .htaccess
<IfModule mod_mime.c>

  # --- Brotli compressed builds (.unityweb) ---
  AddEncoding br .unityweb
  AddType application/octet-stream .data.unityweb
  AddType application/wasm .wasm.unityweb
  AddType application/javascript .js.unityweb

  # --- Gzip compressed builds (if Unity used gzip) ---
  # AddEncoding gzip .unityweb
  # AddType application/octet-stream .data.unityweb
  # AddType application/wasm .wasm.unityweb
  # AddType application/javascript .js.unityweb

  # --- For uncompressed builds ---
  AddType application/wasm .wasm

</IfModule>
